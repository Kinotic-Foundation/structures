{
    "name": "@kinotic/structures-cli",
    "version": "2.1.1",
    "description": "Structures CLI provides the ability to interact with the Structures Server",
    "author": "Kinotic Developers",
    "bin": {
        "structures": "./bin/run.js"
    },
    "homepage": "https://kinotic-foundation.github.io/structures/",
    "repository": "https://github.com/Kinotic-Foundation/structures",
    "license": "MIT",
    "main": "dist/index.js",
    "type": "module",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@kinotic/continuum-client": "^2.10.5",
        "@kinotic/continuum-idl": "^2.0.0",
        "@kinotic/structures-api": "file:.yalc/@kinotic/structures-api",
        "@oclif/core": "^4.0.33",
        "@oclif/plugin-autocomplete": "^3.2.10",
        "@oclif/plugin-help": "^6",
        "@oclif/plugin-not-found": "^3.2.28",
        "@oclif/plugin-plugins": "^5.4.17",
        "@oclif/plugin-update": "^4.6.13",
        "@oclif/plugin-warn-if-update-available": "^3.1.23",
        "chalk": "^5.3.0",
        "inquirer": "^12.1.0",
        "liquidjs": "^10.19.0",
        "open": "^10.1.0",
        "p-timeout": "^6.1.3",
        "radash": "^12.1.0",
        "terminal-link": "^3.0.0",
        "ts-morph": "^24.0.0",
        "uuid": "^11.0.3",
        "ws": "^8.18.0",
        "yaml": "^2.6.1"
    },
    "devDependencies": {
        "@oclif/test": "^4.1.2",
        "@types/chai": "^5",
        "@types/inquirer": "^9.0.7",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.9.1",
        "@types/uuid": "^10.0.0",
        "@types/ws": "^8.5.13",
        "chai": "^5",
        "eslint": "^8.57.1",
        "eslint-config-oclif": "^5",
        "eslint-config-oclif-typescript": "^3.1.12",
        "mocha": "^10",
        "oclif": "^4.15.28",
        "shx": "^0.3.4",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2"
    },
    "oclif": {
        "bin": "structures",
        "dirname": "structures",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins",
            "@oclif/plugin-update",
            "@oclif/plugin-not-found",
            "@oclif/plugin-warn-if-update-available",
            "@oclif/plugin-autocomplete"
        ],
        "topicSeparator": " ",
        "topics": {}
    },
    "scripts": {
        "build": "shx rm -rf dist && tsc -b && pnpm run copy-files",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "pnpm lint",
        "prepack": "pnpm build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "copy-files": "cp -r ./src/templates/  ./dist/templates/"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/https://github.com/Kinotic-Foundation/structures/structures/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts",
    "volta": {
        "node": "18.17.0"
    }
}
