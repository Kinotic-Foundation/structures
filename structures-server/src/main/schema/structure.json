{ 
  "$schema": "http://json-schema.org/draft-07/schema#", 
  "$id": "https://yourdomain.com/schemas/personality.json",
  "type": "object",
  "properties": { 
    "id": { "type": "string" }, 
    "name": { "type": "string", "pattern": "^.*$" }, 
    "description": { "type": "string" }, 
    "created": { "type": "number" }, 
    "updated": { "type": "number" }, 
    "traits": {  
       "type": "array",  
       "uniqueItems": true,  
       "items": { 
           "$ref": "trait.json"
       } 
    } 
  }, 
  "required": ["name","description","created","updated","traits"] 
}